<Window x:Class="_4phn.View.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4phn.View"
        mc:Ignorable="d"
        Title="О программе" Height="259" Width="250" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Image x:Name="image" Source="/Resources/call.ico" Height="100" Margin="20"/>
        <TextBlock x:Name="tbName" TextWrapping="Wrap" TextAlignment="Center" FontSize="20"/>
        <Button x:Name="button" Content="OK" Margin="50 10 55 0" Height="35" Click="Button_Click"/>


    </StackPanel>
</Window>
